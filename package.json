{
  "name": "personal-site",
  "version": "1.4.0",
  "private": true,
  "description": "matthewbarton.dev site",
  "author": "mdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatthewNavy/personal-site.git"
  },
  "bugs": {
    "url": "https://github.com/twbs/bootstrap-npm-starter/issues"
  },
  "homepage": "https://github.com/twbs/bootstrap-npm-starter#readme",
  "scripts": {
    "build": "npm run public/css",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 public/scss -o public/assets/css/",
    "css-lint": "stylelint public/scss/",
    "css-prefix": "postcss --replace public/assets/css/starter.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css public/assets/css/starter.css --content public/index.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output public/assets/css/",
    "css": "npm-run-all css-compile css-prefix",
    "server": "sirv --port 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm run css-lint && npm run css"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "dependencies": {
    "bootstrap": "^5.0.0-beta3",
    "bootstrap-icons": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jquery": "^3.6.0",
    "nodemailer": "^6.5.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.2",
    "sirv-cli": "^1.0.11",
    "stylelint": "^13.12.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0"
  }
}
